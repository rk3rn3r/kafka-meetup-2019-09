# App
APP_NAME=tracking-demo
APP_CONFLUENT_PLATFORM_VERSION=5.3.1
TRACKING_TOPICS=tracking
HTTP_SINK_NAME=tracking-http-sink-meetup201909
ANALYTICS_API_KEY=FhDv5sp19A8ml

# Zookeeper
ZOOKEEPER_CLIENT_PORT=2181
ZOOKEEPER_TICK_TIME=2000
ZOOKEEPER_INIT_LIMIT=5
ZOOKEEPER_SYNC_LIMIT=2
ZOOKEEPER_SERVERS=meetup201909-zookeeper01:22888:23888;meetup201909-zookeeper02:22888:23888;meetup201909-zookeeper03:22888:23888

# Kafka Broker
KAFKA_DEFAULT_REPLICATION_FACTOR=2
