KAFKA_OFFSETS_RETENTION_MINUTES=43200
KAFKA_LOG_CLEANER_DELETE_RETENTION_MS=2592000000
KAFKA_GROUP_INITIAL_REBALANCE_DELAY_MS=0 ## for DEV ONLY!
KAFKA_LOG_ROLL_HOURS=24
KAFKA_CONFLUENT_SUPPORT_METRICS_ENABLE=false

# Kafka Connect
CONNECT_ZOOKEEPER_CONNECT=${KAFKA_ZOOKEEPER_CONNECT}

# Local Broker Connect String
BOOTSTRAP_SERVERS=meetup201909-broker01:29092,meetup201909-broker02:29092,meetup201909-broker03:29092
